@model Exception
@{
    ViewBag.Title = "Błąd";
}

@section navigation{
    @Html.Partial("_Menu", new MenuViewModel() { MyProfileSelected = true })
}
<div class="not-found-container">
    <h1 class="align-center">404 NOT FOUND @Model.Message</h1>
    <h5 class="align-center">Wystąpił błąd. Spróbuj ponownie lub skontaktuj się z Administratorem systemu.</h5>

</div>