@using EwiPraca.Constants
<!DOCTYPE html>

<!--[if IE 7]>                  <html class="ie7 no-js" lang="en">     <![endif]-->
<!--[if lte IE 8]>              <html class="ie8 no-js" lang="en">     <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
<html class="not-ie no-js" lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <title>@ViewBag.Title - EwiPraca</title>

    <meta name="description" content="">
    <meta name="author" content="">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/jquery-1.7.1.min.js"><\/script>')</script>
    @Scripts.Render("~/bundles/datatables")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/bootstrap")
    <!--[if !lte IE 6]><!-->
    <link rel="stylesheet" href="~/Content/Site.css" media="screen" />

    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:400,600,300,800,700,400italic|PT+Serif:400,400italic" />
    <link rel="stylesheet" href="~/Content/fancybox.min.css" media="screen" />
    <link rel="stylesheet" href="~/Content/audioplayerv1.min.css" media="screen" />
    <!--<![endif]-->
    <!--[if lte IE 6]>
        <link rel="stylesheet" href="//universal-ie6-css.googlecode.com/files/ie6.1.1.css" media="screen, projection">
    <![endif]-->
    <!-- HTML5 Shiv + detect touch events -->
    <script src="~/Scripts/modernizr.custom.js"></script>

</head>
<body>

    <header id="header" class="container clearfix">

        <a href="@Url.Action("Index", "Home")" id="logo">
            <img src="~/Content/img/logo.png" alt="EwiPraca">
        </a>

        @RenderSection("navigation")

    </header>

    <section id="content" class="container clearfix">

        @RenderBody()

    </section><!-- end #content -->

   

    <footer id="footer" class="clearfix">

        <div class="container">
            <div class="one-fourth last">

                <span class="title">Social media</span>

                <ul class="social-links">
                    <li class="twitter"><a href="#">Twitter</a></li>
                    <li class="facebook"><a href="#">Facebook</a></li>
                    <li class="digg"><a href="#">Digg</a></li>
                    <li class="vimeo"><a href="#">Vimeo</a></li>
                    <li class="youtube"><a href="#">YouTube</a></li>
                    <li class="skype"><a href="#">Skype</a></li>
                </ul><!-- end .social-links -->

            </div><!-- end .one-fourth.last -->

        </div><!-- end .container -->

    </footer><!-- end #footer -->

    <footer id="footer-bottom" class="clearfix">

        <div class="container">

            <ul>
                <li>EwiPraca &copy; 2018</li>
                <li><a href="#">Warunki</a></li>
            </ul>

        </div><!-- end .container -->

    </footer><!-- end #footer-bottom -->
      <div class="loader-container">
        <svg class="loader" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 340 340">
            <circle cx="170" cy="170" r="160" stroke="#f15a23" />
            <circle cx="170" cy="170" r="135" stroke="#404041" />
            <circle cx="170" cy="170" r="110" stroke="#f15a23" />
            <circle cx="170" cy="170" r="85" stroke="#404041" />
        </svg>
    </div>

    @RenderSection("scripts", required: false)

    <!--[if !lte IE 6]><!-->
    <!--[if lt IE 9]> <script src="js/selectivizr-and-extra-selectors.min.js"></script> <![endif]-->
    <script src="~/Scripts/respond.min.js"></script>
    <script src="~/Scripts/jquery.easing-1.3.min.js"></script>
    <script src="~/Scripts/jquery.fancybox.pack.js"></script>
    <script src="~/Scripts/jquery.smartStartSlider.min.js"></script>
    <script src="~/Scripts/jquery.jcarousel.min.js"></script>
    <script src="~/Scripts/jquery.cycle.all.min.js"></script>
    <script src="~/Scripts/jquery.isotope.min.js"></script>
    <script src="~/Scripts/audioplayerv1.min.js"></script>
    <script src="//maps.google.com/maps/api/js?sensor=false"></script>
    <script src="~/Scripts/jquery.gmap.min.js"></script>
    <script src="~/Scripts/jquery.touchSwipe.min.js"></script>
    <script src="~/Scripts/custom.js"></script>
    <!--<![endif]-->
</body>
</html>